<h1>Editing role_permission</h1>

<% form_for(@role_permission) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :role_id %><br />
    <%= f.text_field :role_id %>
  </p>
  <p>
    <%= f.label :permission_id %><br />
    <%= f.text_field :permission_id %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @role_permission %> |
<%= link_to 'Back', role_permissions_path %>

