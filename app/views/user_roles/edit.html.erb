<h1>Editing user_role</h1>

<% form_for(@user_role) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :id_user %><br />
    <%= f.text_field :id_user %>
  </p>
  <p>
    <%= f.label :id_role %><br />
    <%= f.text_field :id_role %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @user_role %> |
<%= link_to 'Back', user_roles_path %>
